{
    "name": "ziyaretci-takip",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "^6.0.0",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^13.5.0",
        "antd": "^5.26.6",
        "axios": "^1.10.0",
        "jspdf": "^3.0.1",
        "jspdf-autotable": "^5.0.2",
        "jwt-decode": "^4.0.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.7.0",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4",
        "xlsx": "^0.18.5"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "react-typescript-scripts": "0.0.4",
        "webpack": "^5.101.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "preprocessor.js"
        },
        "testRegex": "/src/__tests__/.*\\.(ts|tsx|js)$",
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileTransform.js",
            "\\.(css|less)$": "<rootDir>/__mocks__/cssTransform.js"
        }
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    }
}
