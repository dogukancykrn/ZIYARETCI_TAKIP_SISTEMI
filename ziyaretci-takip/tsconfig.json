{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5",
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "skipLibCheck": true,
        "strict": false
    },
    "exclude": [
        "node_modules",
        "**/*.test.tsx",
        "src/components/BankLogo.tsx"
    ],
    "include": [
        "src"
    ]
}